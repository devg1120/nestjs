
curl -X POST http://localhost:3000/users \
  -H "Content-Type: application/json" \
  -d '{"name": "Alice2", "email": "alice2@prisma.io"}'

curl -X GET http://localhost:3000/users \
  -H "Content-Type: application/json" 

curl  http://localhost:3000/users 


